-- carregar biblioteca
local Fluent = loadstring(game:HttpGet("https://github.com/dawid-scripts/Fluent/releases/latest/download/main.lua"))()

local Window = Fluent:CreateWindow({
    Title = "kaka hub" .. Fluent.Version,
    TabWidth = 160, Size = UDim2.fromOffset(580, 460), Theme = "Dark"
})

local Tabs = {
    Main = Window:AddTab({ Title = "Main", Icon = "home"}),
    Settings = Window:AddTab({ Title = "Settings", Icon = "settings" }), 
    Bedwars = Window:AddTab({ Title = "bedwars", Icon = "bed"}), 
    Volleyball = Window:AddTab({ Title = "volleyball", Icon = "ball"}), 
    Bloxfruits = Window:AddTab({ Title = "blox fruits", Icon = "apple"}), 
    Deadrails  = Window:AddTab({ Title = "dead rails", Icon = "skull"}), 
    Fish = Window:AddTab({ Title = "fish", Icon = "sea"}), 
    Animepower = Window:AddTab({ Title = "anime power", Icon = "home"}), 
    Doors = Window:AddTab({ Title = "doors", Icon = "door"}), 
    Viagemempueirada = Window:AddTab({ Title = "viagemempueirada", Icon = "home"}), 
    Lendasninja = Window:AddTab({ Title = "lendas ninja", Icon = "sword"}), 
    Bluelock = Window:AddTab({ Title = "blue lock", Icon = "boll"}), 
    Jujutsuinfinite = Window:AddTab({ Title = "jujutsu infinite", Icon = "home"}), 
    Ninjatime = Window:AddTab({ Title = "ninja time", Icon = "sword"}), 
    Malldrifters = Window:AddTab({ Title = "mall drifters", Icon = "home"}), 
    Musclelegends = Window:AddTab({ Title = "Muscle legends", Icon = "muscle"}), 
    Thestrongestbattlegrounds = Window:AddTab({ Title = "The strongest battlegrounds", Icon = "home"}),
    Animevanguards = Window:AddTab({ Title = "Anime vanguards", Icon = "home"}),
    PetGO = Window:AddTab({ Title = "Pet GO", Icon = "home"}),
    Animereborn = Window:AddTab({ Title = "Anime reborn", Icon = "home"}),
    Arisecrossover = Window:AddTab({ Title = "Arise crossover", Icon = "home"}),
}
